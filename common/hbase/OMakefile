
RESULT = hbase
RESULTS = $(RESULT).cmxa

OCAML_LIBS += $(UBASEDIR)/ubase 
OCAMLINCLUDES += $(UBASEDIR) 

HBASE_LIBRARY_SOURCES[] = \
  int.ml error.ml info.ml misc.ml mapplus.ml env.ml name.ml memo.ml 

OCamlLibrary($(RESULT), $(rootname $(HBASE_LIBRARY_SOURCES)))

.DEFAULT: $(RESULTS)

clean: 
	$(CLEAN) $(RESULT)