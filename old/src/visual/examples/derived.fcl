let hoist_hd p = hoist_nonunique *h p;hoist_nonunique *t (neg p)
let map_head l = fork {*h} (map l) id
let map_tail l = fork {*t} (map l) id
let mapp p l = fork p (map l) id

let promote k1 k2 =
  fork {k1}
    (map (fork {k2} id (plunge *temp*)))
    id;
  hoist_nonunique k1 {*temp* k2};
  rename *temp* k1

do id