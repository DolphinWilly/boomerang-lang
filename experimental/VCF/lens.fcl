module Lens=

type value = { "PCDATA" = `Any :: [] }

let lensCommon: lens = 
  acond value <{ ! = {} }>
  (hoist "PCDATA"; List. hd [])
    ( map (List.hd []; hoist ""; flatten;
	   plunge "_"; wmap {"_" -> lensCommon}; hoist "_") )

let l = 
  hoist ""; List.hd []; 
  map (hoist ""; flatten; lensCommon)

